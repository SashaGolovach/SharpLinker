{
    "name": "SharpLinker",
    "version": "0.1",
    "description": "Navigate c# code files easily",
    "manifest_version": 2,
    "icons": [
        {
            "128": "assets/images/icon.png"
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*.cs"
            ],
            "all_frames": true,
            "js": [
                "assets/js/contentScript.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "assets/js/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "https://some-random-api.ml/*"
    ]
}