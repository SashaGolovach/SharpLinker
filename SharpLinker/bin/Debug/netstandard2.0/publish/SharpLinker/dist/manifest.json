{
    "name": "SharpLinker",
    "version": "0.1",
    "description": "Navigate c# code files easily",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "file:///*.html"
            ],
            "all_frames": true,
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "permissions": [
        "http://10.10.10.131:8080/*"
    ]
}                                                                                                                                                       